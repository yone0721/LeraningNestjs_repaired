#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/@nestjs+cli@11.0.3_@swc+cli@0.6.0_@swc+core@1.10.17_chokidar@4.0.3__@swc+core@1.10.17_@types+node@22.13.4/node_modules/@nestjs/cli/bin/node_modules:/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/@nestjs+cli@11.0.3_@swc+cli@0.6.0_@swc+core@1.10.17_chokidar@4.0.3__@swc+core@1.10.17_@types+node@22.13.4/node_modules/@nestjs/cli/node_modules:/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/@nestjs+cli@11.0.3_@swc+cli@0.6.0_@swc+core@1.10.17_chokidar@4.0.3__@swc+core@1.10.17_@types+node@22.13.4/node_modules/@nestjs/node_modules:/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/@nestjs+cli@11.0.3_@swc+cli@0.6.0_@swc+core@1.10.17_chokidar@4.0.3__@swc+core@1.10.17_@types+node@22.13.4/node_modules:/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/@nestjs+cli@11.0.3_@swc+cli@0.6.0_@swc+core@1.10.17_chokidar@4.0.3__@swc+core@1.10.17_@types+node@22.13.4/node_modules/@nestjs/cli/bin/node_modules:/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/@nestjs+cli@11.0.3_@swc+cli@0.6.0_@swc+core@1.10.17_chokidar@4.0.3__@swc+core@1.10.17_@types+node@22.13.4/node_modules/@nestjs/cli/node_modules:/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/@nestjs+cli@11.0.3_@swc+cli@0.6.0_@swc+core@1.10.17_chokidar@4.0.3__@swc+core@1.10.17_@types+node@22.13.4/node_modules/@nestjs/node_modules:/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/@nestjs+cli@11.0.3_@swc+cli@0.6.0_@swc+core@1.10.17_chokidar@4.0.3__@swc+core@1.10.17_@types+node@22.13.4/node_modules:/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nest.js" "$@"
else
  exec node  "$basedir/../../bin/nest.js" "$@"
fi
