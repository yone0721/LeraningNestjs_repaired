#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.10.17_@types+node@22.13.4_typescript@5.7.3/node_modules/ts-node/dist/node_modules:/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.10.17_@types+node@22.13.4_typescript@5.7.3/node_modules/ts-node/node_modules:/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.10.17_@types+node@22.13.4_typescript@5.7.3/node_modules:/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.10.17_@types+node@22.13.4_typescript@5.7.3/node_modules/ts-node/dist/node_modules:/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.10.17_@types+node@22.13.4_typescript@5.7.3/node_modules/ts-node/node_modules:/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.10.17_@types+node@22.13.4_typescript@5.7.3/node_modules:/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin-transpile.js" "$@"
else
  exec node  "$basedir/../../dist/bin-transpile.js" "$@"
fi
