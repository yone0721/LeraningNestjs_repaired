#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/webpack@5.98.0_@swc+core@1.10.17/node_modules/webpack/bin/node_modules:/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/webpack@5.98.0_@swc+core@1.10.17/node_modules/webpack/node_modules:/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/webpack@5.98.0_@swc+core@1.10.17/node_modules:/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/webpack@5.98.0_@swc+core@1.10.17/node_modules/webpack/bin/node_modules:/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/webpack@5.98.0_@swc+core@1.10.17/node_modules/webpack/node_modules:/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/webpack@5.98.0_@swc+core@1.10.17/node_modules:/home/yone0721/handson/nestjs_handson/blog-api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/webpack.js" "$@"
else
  exec node  "$basedir/../../bin/webpack.js" "$@"
fi
